{
  "name": "@arbiter/importer",
  "version": "0.1.0",
  "description": "Codebase analysis and artifact detection for importing existing projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./scanner": {
      "types": "./dist/scanner.d.ts",
      "import": "./dist/scanner.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "bun test",
    "lint": "biome check src/",
    "format": "biome format --write src/"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "importer",
    "codebase-analysis",
    "artifact-detection",
    "plugin-system",
    "brownfield"
  ],
  "author": "Arbiter Team",
  "license": "MIT"
}
