{
  "name": "@arbiter/validation",
  "version": "0.1.0",
  "description": "Spec validation layer for Arbiter - validates and transforms specification data",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
