{
  "name": "@arbiter/api-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Arbiter API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": [
    "arbiter",
    "api",
    "types",
    "typescript"
  ],
  "author": "Nathan Rice",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.9.8"
  }
}
