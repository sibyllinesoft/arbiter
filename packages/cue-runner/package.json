{
  "name": "@arbiter/cue-runner",
  "version": "0.1.0",
  "description": "Shared helpers for invoking CUE tooling (vet/export/fmt) with structured diagnostics",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "bun-types": "^1.2.21",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "cue",
    "vet",
    "export",
    "arbiter",
    "cli",
    "api"
  ],
  "author": "Nathan Rice",
  "license": "MIT"
}
