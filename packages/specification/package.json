{
  "name": "@arbiter/specification",
  "version": "1.0.0",
  "description": "Specification types and utilities for Arbiter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "arbiter",
    "specification",
    "types",
    "utilities"
  ],
  "author": "Nathan Rice",
  "license": "MIT",
  "arbiter": {
    "profiles": [
      "library"
    ],
    "coverage": {
      "threshold": 0.8
    }
  }
}
