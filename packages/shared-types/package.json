{
  "name": "@arbiter/shared-types",
  "version": "1.0.0",
  "description": "Centralized type definitions for Arbiter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./api/*": {
      "types": "./dist/api/*",
      "import": "./dist/api/*"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": [
    "arbiter",
    "types",
    "shared"
  ],
  "author": "Nathan Rice",
  "license": "MIT"
}
