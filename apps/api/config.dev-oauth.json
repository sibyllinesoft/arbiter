  "websocket": { "max_connections": 100, "ping_interval_ms": 30000 },
  "oauth": {
    "enabled": true,
    "mcpBaseUrl": "http://localhost:5050",
    "authServerUrl": "http://localhost:4571",
    "authServerPort": 4571,
    "enableAuthServer": false,
    "requiredScopes": ["read", "write"],
    "provider": "dev-oauth",
    "clientId": "dev-cli",
    "redirectUri": "http://localhost:3000/oauth/callback"
  }
}
