{
  "port": 5050,
  "host": "0.0.0.0",
  "database_path": "arbiter.dev-oauth.db",
  "spec_workdir": ".spec-workdir",
  "jq_binary_path": "jq",
  "auth_required": true,
  "rate_limit": { "max_tokens": 10, "refill_rate": 1, "window_ms": 10000 },
