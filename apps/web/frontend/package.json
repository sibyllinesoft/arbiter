{
  "name": "spec-workbench-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:cue-stories": "playwright test tests/cue-*.spec.ts",
    "test:comprehensive": "playwright test tests/test-runner.spec.ts",
    "storybook": "storybook dev -p 6007",
    "storybook:test": "concurrently \"npm run storybook\" \"wait-on http://localhost:6007 && npm run test:cue-stories\"",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.0",
    "@hpcc-js/wasm": "^2.13.0",
    "@monaco-editor/react": "^4.6.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@types/react-select": "^5.0.1",
    "@types/react-virtualized": "^9.21.29",
    "@xstate/graph": "^2.0.0",
    "clsx": "^2.0.0",
    "d3": "^7.9.0",
    "lucide-react": "^0.294.0",
    "mermaid": "^10.6.1",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.10.2",
    "react-split-pane": "^0.1.92",
    "react-tabs": "^6.0.2",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.2",
    "react-virtualized": "^9.22.5",
    "vis-data": "^8.0.1",
    "vis-network": "^10.0.1",
    "xstate": "^5.0.0",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.10",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^1.0.4",
    "wait-on": "^8.0.4"
  }
}
