{
  "$schema": "https://www.getmaxim.ai/schema",
  "client": {
    "drop_excess_requests": false,
    "enable_logging": true,
    "initial_pool_size": 256,
    "prometheus_labels": ["handler", "environment"],
    "allow_direct_keys": true,
    "log_level": "debug",
    "log_output_style": "pretty",
    "fast_startup": false
  },
  "providers": {
    "openrouter": {
      "keys": [
        {
          "value": "env.OPENROUTER_API_KEY",
          "models": [
            "anthropic/claude-3.5-sonnet",
            "anthropic/claude-3-5-sonnet",
            "anthropic/claude-3-5-sonnet-20241022",
            "grok-4",
            "grok-4-mini",
            "x-ai/grok-4-fast"
          ],
          "weight": 1
        }
      ],
      "network_config": {
        "extra_headers": {
          "HTTP-Referer": "https://arbiter.local",
          "X-Title": "Arbiter Claude Code Gateway"
        }
      }
    }
  },
  "plugins": [
    {
      "name": "telemetry",
      "enabled": true,
      "config": {
        "custom_labels": ["handler", "environment"]
      }
    }
  ]
}
